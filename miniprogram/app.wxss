/**app.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

.round {
  padding: 20px;
  border-radius: 20px;
  box-shadow: 0 0 10px 2px #bdcee0;
  box-sizing: border-box;
}

.center {
  display: flex;
  justify-content: center;
  margin-top: 30rpx;
}

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}


page {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}